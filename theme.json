{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,

  "settings": {
    "appearanceTools": true,
    "color": {
      "palette": [
        {
          "slug": "primary",
          "color": "#DAD8CC",
          "name": "Beige"
        },
        {
          "slug": "secondary",
          "color": "#F5F5F5",
          "name": "Grå"
        },
        {
          "slug": "blue",
          "color": "#335987",
          "name": "Royal blue"
        },
        {
          "slug": "teal",
          "color": "#437181",
          "name": "Teal"
        },
        {
          "slug": "olive",
          "color": "#938D4C",
          "name": "Olive"
        },
        {
          "slug": "Golden",
          "color": "#C09440",
          "name": "Golden"
        },
        {
          "slug": "red",
          "color": "#B02B08",
          "name": "Red"
        },
        {
          "slug": "light-blue",
          "color": "#99ACC3",
          "name": "Norra ljusblå"
        },
        {
          "slug": "light-blue-2",
          "color": "#D6DEE7",
          "name": "Norra ljusblå 2"
        },
        {
          "slug": "light-teal-1",
          "color": "#A1B8C0",
          "name": "Västra light teal"
        },
        {
          "slug": "light-teal-2",
          "color": "#D9E3E6",
          "name": "Västra light teal 2"
        },
        {
          "slug": "light-olive",
          "color": "#C9C6A5",
          "name": "Mellansvenska Light olive"
        },
        {
          "slug": "light-olive-2",
          "color": "#E9E8DB",
          "name": "Mellansvenska Light olive 2"
        },
        {
          "slug": "light-golden",
          "color": "#DFC99F",
          "name": "Södra Light golden"
        },
        {
          "slug": "light-golden 2",
          "color": "#F2EAD9",
          "name": "Södra Light golden-2"
        }
      ]
    },
    "layout": {
      "contentSize": "840px",
      "wideSize": "1100px"
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "\"Barlow\", sans-serif",
          "name": "Barlow",
          "slug": "body",
          "fontFace": [
            {
              "fontFamily": "Barlow",
              "fontWeight": "100 200 300 400 500 600 700 800 900",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "//fonts.googleapis.com/css2?family=Barlow:wght@100;200;300;400;500;600;700;800;900&display=swap"
              ]
            }
          ]
        }
      ]
    }
  },
  "styles": {
    "blocks": {
      "core/button": {
        "color": {
          "text": "#000000"
        }
      }
    },
    "elements": {
      "link": {
        "typography": {
          "textDecoration": "none"
        },
        ":hover": {
          "typography": {
            "textDecoration": "none"
          }
        }
      },
      "h1": {
        "typography": {
          "fontWeight": "400",
          "textTransform": "uppercase"
        }
      }
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    },
    {
      "area": "uncategorized",
      "name": "sidebar",
      "title": "Sidebar"
    },
    {
      "area": "uncategorized",
      "name": "post-meta",
      "title": "Post Meta"
    }
  ],
  "customTemplates": [
    {
      "name": "template-norra-frontpage",
      "title": "Norra Avdelning Frontpage"
    },
    {
      "name": "template-vastra-frontpage",
      "title": "Västra Avdelning Frontpage"
    },
    {
      "name": "template-mellansvenska-frontpage",
      "title": "Mellansvenska Avdelning Frontpage"
    },
    {
      "name": "template-sodra-frontpage",
      "title": "Södra Avdelning Frontpage"
    },
    {
      "name": "single-vastra",
      "title": "Västra"
    },
    {
      "name": "single-hanvisningssida",
      "title": "Hänvisningssida"
    },
    {
      "name": "single-omplacering",
      "title": "Omplaceringssida"
    }
  ]
}
